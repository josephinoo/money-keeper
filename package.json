{
  "name": "client",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "start": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:codegen": "npx playwright codegen http://localhost:5173",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "tsc --noemit && prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "generate:icons": "node ./pack-icons.js && prettier --write ./src/lib/icons.ts",
    "generate:prisma": "npx prisma generate",
    "generate": "npm run generate:icons && npm run generate:prisma",
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.18",
    "@iconify/svelte": "^3.1.0",
    "@iconify/utils": "^2.1.0",
    "@playwright/test": "^1.30.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.1.7",
    "@sveltejs/kit": "^1.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vite-pwa/sveltekit": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.3",
    "prettier-plugin-svelte": "^2.9.0",
    "prisma": "^4.9.0",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.3",
    "svelte-i18n": "^3.6.0",
    "svelte-preprocess": "^5.0.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.1"
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "bcrypt": "^5.1.0",
    "dayjs": "^1.11.7",
    "idb": "^7.1.1",
    "uuid": "^9.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "type": "module"
}
