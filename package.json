{
  "name": "client",
  "version": "2.8.4",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "start": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:codegen": "npx playwright codegen http://localhost:5173",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "tsc --noemit && prettier --check . && eslint .",
    "format": "prettier --write .",
    "generate:icons": "node ./pack-icons.js && prettier --write ./src/lib/icons.ts",
    "generate:prisma": "npx prisma generate",
    "generate": "npm run generate:icons && npm run generate:prisma"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.176",
    "@iconify/svelte": "^3.1.6",
    "@iconify/utils": "^2.1.20",
    "@playwright/test": "^1.41.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/adapter-node": "^4.0.1",
    "@sveltejs/kit": "^2.5.2",
    "@types/bcrypt": "^5.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vite-pwa/sveltekit": "^0.3.0",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "prisma": "^5.8.1",
    "svelte": "^4.2.9",
    "svelte-chartjs": "^3.1.5",
    "svelte-check": "^3.6.3",
    "svelte-i18n": "^4.0.0",
    "svelte-preprocess": "^5.1.3",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "@vercel/analytics": "^1.1.2",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.10",
    "idb": "^8.0.0",
    "svelte-dnd-action": "^0.9.38",
    "timezones-list": "^3.0.3",
    "uuid": "^9.0.1"
  },
  "type": "module"
}
